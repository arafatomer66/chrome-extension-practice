{
        "manifest_version": 2,
        "name": "Facebook Filler",
        "description": "Facebook Filler",
        "version": "1.0",
        "browser_action": {
            "default_title": "Click me",
            "default_popup": "index.html"
        },
        "permissions": [
            "tabs",
            "http://*/*",
            "https://*/*"
        ]
    }